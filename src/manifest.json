{
	"manifest_version": 2,
	"name": "Play Me",
	"description": "Chrome Extension for Play Me",
	"version": "0.0.3.2",
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "index.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"http://music.trademe.local/*/*",
		"http://guesswho/*/*",
		"http://chc-music.trademe.local/*/*"
	]
}