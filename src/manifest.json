{
	"manifest_version": 2,
	"name": "Play Me",
	"description": "Chrome Extension for Play Me",
	"version": "2.0.1.2",
	"browser_action": {
		"default_icon": "assets/images/icon-inverted.png",
		"default_popup": "index.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"identity",
		"https://accounts.spotify.com/authorize",
		"https://api.spotify.com/*",
		"http://music.trademe.local/*/*",
		"http://guesswho/*/*",
		"http://chc-music.trademe.local/*/*",
		"https://fonts.googleapis.com/icon"
	]
}
